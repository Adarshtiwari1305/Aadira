{
  "name": "Dr Music",
  "description": "Dr Music Is A Telegram Bot Project That's Allow You To Play Music On Telegram Voice Chat Group.",
  "keywords": ["music", "voicechat", "telegram"],
  "logo": "https://telegra.ph/file/336e006861a2667a7663f.jpg",
  "repository": "https://github.com/jankarikiduniya/Dr-Music",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string get it from @Pyrogram_String_Bot",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot Name.",
      "required": false,
      "value": ""
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "your heroku api key get it from https://dashboard.heroku.com/account",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Your heroku appname",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "400"
    },
    "BOT_USERNAME": {
      "description": "USERNAME BOT",
      "required": true
    },
    "OWNER_ID": {
      "description": "Owner userid",
      "required": true
    },
    "ASSISTANT_USERNAME": {
      "description": "USERNAME ASSISTANT",
      "required": true
    }     
  }
}
